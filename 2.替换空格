#1.
class Solution:
    # s 源字符串
    def replaceSpace(self, s):
        # write code here
        str = ""
        for i in s:
            if i == " ":
                i = "%20"
                str += i
            else:
                str += i
        return str
#2.
class Solution:
    # s 源字符串
    def replaceSpace(self, s):
        # write code here
        import re
        return re.sub('\s','%20',s)
